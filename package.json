{
  "name": "@ubiquity-os/plugin-sdk",
  "version": "3.6.1",
  "description": "SDK for plugin support.",
  "author": "Ubiquity DAO",
  "license": "MIT",
  "engines": {
    "node": ">=24.11.0"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "manifest": [
        "dist/manifest.d.ts"
      ],
      "constants": [
        "dist/constants.d.ts"
      ],
      "signature": [
        "dist/signature.d.ts"
      ],
      "octokit": [
        "dist/octokit.d.ts"
      ],
      "helpers": [
        "dist/helpers.d.ts"
      ],
      "compression": [
        "dist/compression.d.ts"
      ],
      "configuration": [
        "dist/configuration.d.ts"
      ],
      "llm": [
        "dist/llm.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "import": "./dist/manifest.mjs",
      "require": "./dist/manifest.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    },
    "./signature": {
      "types": "./dist/signature.d.ts",
      "import": "./dist/signature.mjs",
      "require": "./dist/signature.js"
    },
    "./octokit": {
      "types": "./dist/octokit.d.ts",
      "import": "./dist/octokit.mjs",
      "require": "./dist/octokit.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.mjs",
      "require": "./dist/helpers.js"
    },
    "./compression": {
      "types": "./dist/compression.d.ts",
      "import": "./dist/compression.mjs",
      "require": "./dist/compression.js"
    },
    "./configuration": {
      "types": "./dist/configuration.d.ts",
      "import": "./dist/configuration.mjs",
      "require": "./dist/configuration.js"
    },
    "./llm": {
      "types": "./dist/llm.d.ts",
      "import": "./dist/llm.mjs",
      "require": "./dist/llm.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "run-p worker proxy",
    "format": "run-s format:*",
    "format:lint": "eslint --fix .",
    "format:prettier": "prettier --write .",
    "format:cspell": "cspell **/*",
    "jest:test": "jest --coverage",
    "knip": "knip --config .github/knip.ts",
    "knip-ci": "knip --no-exit-code --reporter json --config .github/knip.ts",
    "prepare": "node .husky/install.mjs",
    "prebuild": "rimraf dist"
  },
  "keywords": [
    "typescript",
    "template",
    "dao",
    "ubiquity",
    "open-source"
  ],
  "dependencies": {
    "@octokit/auth-app": "^7.1.1",
    "@octokit/core": "^6.1.3",
    "@octokit/plugin-paginate-rest": "^11.4.2",
    "@octokit/plugin-request-log": "^5.3.1",
    "@octokit/plugin-rest-endpoint-methods": "^13.2.1",
    "@octokit/plugin-retry": "^7.1.2",
    "@octokit/plugin-throttling": "^9.3.2",
    "@octokit/rest": "^21.0.2",
    "@octokit/types": "^13.6.1",
    "@octokit/webhooks": "^13.4.1",
    "@octokit/webhooks-types": "^7.6.1",
    "@sinclair/typebox": "^0.33.17",
    "libsodium-wrappers": "^0.7.15",
    "openai": "^4.70.2",
    "pino": "^9.7.0",
    "typebox-validators": "^0.3.5",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@cspell/dict-node": "^5.0.8",
    "@cspell/dict-software-terms": "^4.2.1",
    "@cspell/dict-typescript": "^3.1.6",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.14",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^22.10.1",
    "@ubiquity-os/eslint-plugin-no-empty-strings": "^1.0.3",
    "cspell": "^8.17.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-md-dashboard": "^0.8.1",
    "knip": "^5.69.1",
    "lint-staged": "^15.5.2",
    "msw": "^2.12.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**.{ts,json}": [
      "cspell"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
